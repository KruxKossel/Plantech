@model IEnumerable<Plantech.Models.Colheita>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Quantidade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataColheita)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funcionario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoteHortalica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoteInsumo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plantio)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Quantidade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataColheita)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Funcionario.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoteHortalica.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoteInsumo.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plantio.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
